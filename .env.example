# Configurações do Qbittorrent
QB_URL=http://127.0.0.1:8080
QB_USER=admin
QB_PASS=senha

# Configurações do Bot do Telegram
TELEGRAM_BOT_TOKEN=seu_token_do_bot
TELEGRAM_CHAT_ID=seu_chat_id
INTERVALO=300  # Intervalo em segundos entre verificações
# Caminho de armazenamento do Qbittorrent
QBITTORRENT_STORAGE_PATH=/

# Usuários autorizados (separados por vírgula)
